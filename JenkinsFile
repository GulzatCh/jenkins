pipeline {

 agent any

stages {

 stage('Build') {
     steps {
        sh '''
        docker build -t myawesomecat:${BUILD_NUMBER} -f /awesomecat_backend/Dockerfile
        docker push 
        '''
     }
  }
  stage('Deploy') {
    steps {
        sh 'echo "it was done through jenkins"'
        
    }
    }
  
 } 

}
