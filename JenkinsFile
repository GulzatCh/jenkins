pipeline {

 agent any


 stages {

  stage('Clone') {
     steps {
       branch: 'main' 
       git url: 'https://github.com/GulzatCh/jenkins.git'
   }
  }
  stage('Build') {
     steps {
        sh '''
        docker build -t my awesomecat:${BUILD_NUMBER}
        '''
     }
  }
  stage('Deploy') {
    steps {
        sh 'echo "it was done through jenkins"'
        
    }
  }
 } 

}
